    </servlet>
    <servlet-mapping>
        <servlet-name>Time1</servlet-name>
        <url-pattern>/time1</url-pattern>
    </servlet-mapping>

    <servlet>
       <servlet-name>ShowTime0</servlet-name>
       <description>Pokazuje date i czas</description>
       <servlet-class>ShowTime0</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowTime0</servlet-name>
        <url-pattern>/Time</url-pattern>
    </servlet-mapping>

    <servlet>
       <servlet-name>ShowTime</servlet-name>
       <description>Pokazuje date i czas sesje</description>
       <servlet-class>ShowTime</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowTime</servlet-name>
        <url-pattern>/ShowTime</url-pattern>
    </servlet-mapping>

    <servlet>
       <servlet-name>Params1</servlet-name>
