    </servlet-mapping>

    <servlet>
       <servlet-name>Params2</servlet-name>
       <description>Test parametrow</description>
       <servlet-class>Params2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Params2</servlet-name>
        <url-pattern>/paramtest</url-pattern>
    </servlet-mapping>

    <servlet>
       <servlet-name>ClientTester</servlet-name>
       <description>Test klienta HTTP</description>
       <servlet-class>ClientTester</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClientTester</servlet-name>
        <url-pattern>/clientTester</url-pattern>
    </servlet-mapping>

    <servlet>
       <servlet-name>RegexTest</servlet-name>
       <description>  Regularne wyrazenia 1 zle</description>
       <servlet-class>RegexTest</servlet-class>
       <init-param>
          <param-name>regexFormFile</param-name>
          <param-value>regexform.html</param-value>
       </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegexTest</servlet-name>
        <url-pattern>/regex1 </url-pattern>
    </servlet-mapping>

</web-app>